{
  "name": "circular-iq",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --ssl true --ssl-cert ssl/server.crt --ssl-key ssl/server.key",
    "serve": "ng serve -o  --ssl true --ssl-cert ssl/server.crt --ssl-key ssl/server.key",
    "serve:devmyenergy": "ng serve -o --configuration=devmyenergy --ssl true --ssl-cert ssl/server.crt --ssl-key ssl/server.key",
    "serve:testmyenergy": "ng serve -o --configuration=testmyenergy --ssl true --ssl-cert ssl/server.crt --ssl-key ssl/server.key",
    "serve:acceptmyenergy": "ng serve -o --configuration=acceptmyenergy --ssl true --ssl-cert ssl/server.crt --ssl-key ssl/server.key",
    "serve:prodmyenergy": "ng serve -o --configuration=prodmyenergy --ssl true --ssl-cert ssl/server.crt --ssl-key ssl/server.key",
    "build": "node scripts/version.js && ng build",
    "build:devmyenergy": "node scripts/version.js && ng build --configuration=devmyenergy",
    "build:testmyenergy": "node scripts/version.js && ng build --configuration=testmyenergy",
    "build:acceptmyenergy": "node scripts/version.js && ng build --configuration=acceptmyenergy",
    "build:prodmyenergy": "node scripts/version.js && ng build --configuration=prodmyenergy",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular-devkit/build-optimizer": "^0.1001.2",
    "@angular/animations": "^10.1.2",
    "@angular/cdk": "^10.2.1",
    "@angular/common": "^10.1.2",
    "@angular/compiler": "^10.1.2",
    "@angular/core": "^10.1.2",
    "@angular/forms": "^10.1.2",
    "@angular/http": "^7.2.16",
    "@angular/material": "^10.2.1",
    "@angular/material-moment-adapter": "^12.1.4",
    "@angular/platform-browser": "^10.1.2",
    "@angular/platform-browser-dynamic": "^10.1.2",
    "@angular/router": "^10.1.2",
    "@angular/service-worker": "^10.1.2",
    "@contentful/rich-text-html-renderer": "^15.4.0",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@handsontable/angular": "^6.0.1",
    "@kolkov/angular-editor": "^1.1.4",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@sentry/browser": "^5.24.0",
    "@techiediaries/ngx-textarea-autosize": "^1.3.0",
    "angular-resize-event": "^2.1.0",
    "axios": "^0.21.4",
    "bootstrap": "^4.5.2",
    "core-js": "^3.6.5",
    "element-resize-event": "3.0.3",
    "file-saver": "^2.0.2",
    "filepond": "^4.20.1",
    "filepond-plugin-file-poster": "^2.3.0",
    "filepond-plugin-file-validate-type": "^1.2.5",
    "filepond-plugin-image-preview": "^4.6.4",
    "hammerjs": "^2.0.8",
    "handsontable": "^8.0.0",
    "handsontable-key-value": "^3.0.2",
    "jquery": "^3.5.1",
    "jwt-decode": "^3.0.0",
    "lodash": "^4.17.20",
    "material-design-icons": "^3.0.1",
    "moment": "^2.29.1",
    "ng2-currency-mask": "^9.0.2",
    "ng2-trim-directive": "^2.3.4",
    "ngx-bootstrap": "^6.1.0",
    "ngx-currency": "^2.5.1",
    "ngx-daterangepicker-material": "^4.0.1",
    "ngx-filepond": "^5.0.1",
    "ngx-owl-carousel-o": "^5.0.0",
    "node-json2html": "^2.1.0",
    "node-sass": "^4.14.1",
    "rxjs": "^6.6.3",
    "rxjs-compat": "^6.6.3",
    "storyblok-js-client": "^4.1.5",
    "storyblok-rich-text-react-renderer": "^2.3.1",
    "strip-comments": "^2.0.1",
    "zone.js": "^0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.2",
    "@angular/cli": "~10.1.2",
    "@angular/compiler-cli": "^10.1.2",
    "@angular/language-service": "^10.1.2",
    "@biesbjerg/ngx-translate-extract": "^7.0.2",
    "@ngx-rocket/scripts": "^4.0.0",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.11.1",
    "codelyzer": "^6.0.0",
    "cohere-js": "^1.0.14",
    "git-describe": "^4.0.4",
    "hads": "^2.1.0",
    "htmlhint": "^0.14.1",
    "https-proxy-agent": "^5.0.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "~5.2.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-junit-reporter": "^2.0.1",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "protractor": "~7.0.0",
    "puppeteer": "^5.3.0",
    "stylelint": "~13.7.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "~4.2.0",
    "stylelint-config-standard": "~20.0.0",
    "stylelint-scss": "~3.18.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "~4.0.2"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
